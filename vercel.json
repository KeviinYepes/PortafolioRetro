{
  "builds": [
    {
      "src": "pt_retro/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "buildCommand": "python manage.py collectstatic --noinput"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "pt_retro/wsgi.py"
    }
  ]
}